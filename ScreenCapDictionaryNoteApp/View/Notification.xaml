<Window x:Class="ScreenCapDictionaryNoteApp.View.Notification"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScreenCapDictionaryNoteApp.View"
        mc:Ignorable="d"
        Title="Window1" Width="300"
        >

    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" Margin="4,0,0,0">
            <TextBlock Margin="0 10" Name="MessageTextBlock" TextWrapping="Wrap"/>
            <Button Content="ok" Width="50" Click="OkButton_Clicked" Name="OkButton" Margin="0 0 0 10"/>
        </StackPanel>
    </Grid>
</Window>
